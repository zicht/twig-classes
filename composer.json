{
    "name": "zicht/twig-classes",
    "description": "Twig extension for zicht/classes.",
    "keywords": ["twig", "css"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zicht Online",
            "email": "info@zicht.nl"
        }
    ],
    "require": {
        "twig/twig": "^2.7 || ^3",
        "zicht/classes": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "psr-4": { "Zicht\\Twig\\Extension\\": ["src/"] }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": [
            "@composer install --dev",
            "./vendor/phpunit/phpunit/phpunit"
        ]
    }
}
